import React, { useEffect, useState } from 'react';
import styled from 'styled-components';
import { useSpring, animated } from 'react-spring';

// Sample images for technical skills
const Revit = 'https://via.placeholder.com/30/ff0000/ffffff?text=HTML';
const Naviswork = 'https://via.placeholder.com/30/0000ff/ffffff?text=CSS';
const Autocad = 'https://via.placeholder.com/30/ffff00/000000?text=JS';
const Sketchup = 'https://via.placeholder.com/30/306998/ffffff?text=Py';
const Enscape = 'https://via.placeholder.com/30/306998/ffffff?text=Py';


const Container = styled.div`
  display: flex;
  flex-direction: column;
  align-items: center;
  padding: 50px;
  background : linear-gradient(#1a1f32, #231a2d);
  min-height: 100vh;
`;

const Title = styled.h1`
  color: #fff;
  margin-bottom: 35px;
  margin-bottom: 5px;
`;

const SkillSection = styled.div`
  width: 70%;
`;

const SkillBarContainer = styled.div`
  display: flex;
  align-items: center;
  margin: 35px 0; /* Increase space between skill bars */
  position: relative;
`;

const SkillIcon = styled.img`
  width: 30px;
  height: 30px;
  margin-right: 10px;
`;

const SkillLabel = styled.div`
  margin-bottom: 8px;
   margin-right: 12px;
  font-size: 1.7em;
  color: #fff;
  flex: 1;
`;

const BarContainer = styled.div`
  width: 80%;
  background-color: #333;
  border-radius: 10px;
  overflow: hidden;
  position: relative;
`;

const Bar = styled(animated.div)`
  height: 20px;
  background: linear-gradient(#cf00fe,#9100ff);
  border-radius: 10px;
`;

const BarPercentage = styled.div`
  position: absolute;
  right: 10px;
  top: 50%;
  transform: translateY(-50%);
  color: #fff;
  font-size: 1.5em;
`;

const CircularSkillsContainer = styled.div`
  display: flex;
  justify-content: space-around;
  flex-wrap: wrap;
  margin-top: 35px;
`;

const CircleContainer = styled.div`
  display: flex;
  flex-direction: column;
  align-items: center;
  margin: 25px;
`;

const CircleSVG = styled.svg`
  position: relative;
  width: 128px;
  height: 128px;
  transform: rotate(-90deg);
`;

const BackgroundCircle = styled.circle`
  stroke: #333;
  stroke-width: 8;
`;

const ProgressCircle = styled(animated.circle)`
  stroke: rgba(145, 0, 255);
  stroke-width: 8;
  stroke-linecap: round;
`;

const CircleLabel = styled.div`
  font-size: 1.5em;
  color: #fff;
  margin-top: 10px;
`;

const Percentage = styled.div`
  position: top;
  top: 100%;  /* Move above the circle */
  left: 50%;
  transform: translate(-50%, -50%);
  font-size: 2em;
  color: #00f260;
  font-weight: bold;
  background: rgba(0, 0, 0, 0.6);
  padding: 5px 10px;
  border-radius: 5px;
`;

const SkillBar = ({ label, percentage, icon }) => {
  const [progress, setProgress] = useState(0);

  useEffect(() => {
    setProgress(percentage);
  }, [percentage]);

  const props = useSpring({ width: `${progress}%`, from: { width: '0%' } });

  return (
    <SkillBarContainer>
      <SkillIcon src={icon} alt={`${label} icon`} />
      <SkillLabel>{label}</SkillLabel>
      <BarContainer>
        <Bar style={props} />
        <BarPercentage>{progress}%</BarPercentage>
      </BarContainer>
    </SkillBarContainer>
  );
};

const CircularSkill = ({ label, percentage }) => {
  const [progress, setProgress] = useState(0);

  useEffect(() => {
    setProgress(percentage);
  }, [percentage]);

  const props = useSpring({ value: progress, from: { value: 0 } });

  return (
    <CircleContainer>
      <CircleSVG>
        <BackgroundCircle cx="50%" cy="50%" r="45%" />
        <ProgressCircle
          cx="50%"
          cy="50%"
          r="45%"
          strokeDasharray="283"
          strokeDashoffset={props.value.to((value) => 283 - (283 * value) / 100)}
        />
        
        <Percentage className='one'>{Math.round(progress)}%</Percentage>
      </CircleSVG>
      <CircleLabel>{label}</CircleLabel>
    </CircleContainer>
  );
};

const Skills = () => {
  return (
    <Container>
      <Title>My Skills</Title>
      <SkillSection>
        <SkillBar label="Revit" percentage={90} icon={Revit} />
        <SkillBar label="Naviswork" percentage={50} icon={Naviswork} />
        <SkillBar label="Autocad" percentage={85} icon={Autocad} />
        <SkillBar label="Sketch-up" percentage={50} icon={Sketchup} />
        <SkillBar label="Enscape" percentage={90} icon={Enscape} />
      </SkillSection>
      <CircularSkillsContainer>
        <CircularSkill label="Creativity-90%"  percentage={90} />
        <CircularSkill label="Communication-65%" percentage={65} />
        <CircularSkill label="Problem Solving-75%" percentage={75} />
        <CircularSkill label="Teamwork-85%" percentage={85} />
      </CircularSkillsContainer>
    </Container>
  );
};

export default Skills;



























